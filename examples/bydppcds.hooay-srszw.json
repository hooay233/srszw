		},
		{
			"charactor":"zundamon",
			"style": null,
			"speedScale": 1,
			"pitchScale": 0,
			"intonationScale": 1,
			"volumeScale": 1,
			"prePhonemeLength": 0.1,
			"postPhonemeLength": 0.1,
			"pauseLengthScale": 1,
			"text": {
				"pinyin": null,
				"zi": "我豪爽的答应了"
			}
		},
		{
			"charactor":"zundamon",
